{
  "name": "backend_fsjs",
  "version": "1.0.0",
  "main": "src/app.ts",
  "repository": "git@github.com:spaniernathan/backend_fsjs.git",
  "author": "SPANIER Nathan <spaniernathan67@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": "14.17.5"
  },
  "scripts": {
    "db:start": "docker-compose -f scripts/docker-compose.yml up -d",
    "db:stop": "docker-compose -f scripts/docker-compose.yml stop",
    "db:migrate": "env-cmd -f .env.local npx sequelize db:migrate",
    "db:migrate:undo": "env-cmd -f .env.local npx sequelize db:migrate:undo",
    "dev": "env-cmd -f .env.local npx nodemon --unhandled-rejections=strict --trace-deprecation --config nodemon.json src/app.ts",
    "postinstall": "tsc",
    "start": "node dist/app.js",
    "lint": "eslint . --ext .ts,.js",
    "lint:fix": "eslint . --ext .ts,.js --fix"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "config": "^3.3.6",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-joi": "^0.3.1",
    "http-status-codes": "^2.1.4",
    "joi": "^17.4.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "pg": "^8.7.1",
    "pino": "^6.13.2",
    "sequelize": "^6.6.5",
    "socket.io": "^4.3.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/config": "^0.0.39",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-ws": "^3.0.1",
    "@types/joi": "^17.2.3",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/lodash": "^4.14.173",
    "@types/node": "^16.9.4",
    "@types/pino": "^6.3.11",
    "@types/validator": "^13.6.3",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "env-cmd": "^10.1.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.24.2",
    "sequelize-cli": "^6.2.0",
    "ts-node": "^10.2.1",
    "tsc": "^2.0.3",
    "typescript": "^4.4.3"
  }
}
